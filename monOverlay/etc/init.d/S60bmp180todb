#!/bin/sh
#
# Start the brcmfmac module
#


case "$1" in
  start)
	printf "Starting brcmfmac: "
	/usr/bin/bmp180todb 360000&
	[ $? = 0 ] && echo "OK" || echo "FAIL"
	;;
  stop)
	printf "Stopping brcmfmac: "
	

	[ $? = 0 ] && echo "OK" || echo "FAIL"
	;;
  *)
	echo "Usage: $0 {start|stop}"
	exit 1
esac

exit $?

